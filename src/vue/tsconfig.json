{
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": "../",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"noEmit": true,
		"strict": true,
		"target": "ESNext",
		"types": [
			"gtag.js"
		],
		"paths": {
			"@/*": [
				"./vue/*"
			]
		}
	},
	"include": [
		"./**/*.vue",
		"../js/**/*"
	]
}
